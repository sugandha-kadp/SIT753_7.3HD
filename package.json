{
    "name":  "courseflow",
    "version":  "1.0.0",
    "description":  "A group project using Node.js, MongoDB, and MVC structure.",
    "main":  "src/app.js",
    "scripts":  {
                    "start":  "node src/app.js",
                    "dev":  "nodemon src/app.js",
                    "build":  "npm run test:api",
                    "seed":  "node seedContent.js",
                    "test:api":  "cross-env NODE_ENV=test jest --config testing/config/jest.config.js --runInBand",
                    "test:e2e":  "cross-env NODE_ENV=test playwright test -c testing/config/playwright.config.js",
                    "test:e2e:ci":  "cross-env NODE_ENV=test PLAYWRIGHT_HEADLESS=true CI=1 playwright test -c testing/config/playwright.config.js --reporter=line",
                    "test":  "npm run test:api",
                    "test:ci":  "npm run test:api \u0026\u0026 npm run test:e2e:ci",
                    "test:all":  "npm run test:ci",
                    "report:e2e":  "playwright show-report testing/reports/playwright",
                    "docker:build":  "docker build -t courseflow:local .",
                    "docker:run":  "docker run --rm -p 5000:5000 --env-file .env courseflow:local",
                    "lint":  "eslint \"src/**/*.js\" \"testing/**/*.js\" seedContent.js",
                    "security:audit":  "node scripts/securityAudit.js"
                },
    "dependencies":  {
                         "bcrypt":  "^6.0.0",
                         "bcryptjs":  "^3.0.2",
                         "cloudinary":  "^1.41.3",
                         "connect-mongo":  "^5.1.0",
                         "dotenv":  "^8.6.0",
                         "express":  "^4.21.2",
                         "express-session":  "^1.18.2",
                         "jsonwebtoken":  "^9.0.2",
                         "mongodb":  "^6.19.0",
                         "mongoose":  "^7.6.3",
                         "multer":  "^2.0.2",
                         "multer-storage-cloudinary":  "^4.0.0"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.36.0",
                            "@playwright/test":  "^1.48.2",
                            "cross-env":  "^7.0.3",
                            "eslint":  "^9.36.0",
                            "jest":  "^29.7.0",
                            "jest-html-reporters":  "^3.1.7",
                            "mongodb-memory-server":  "^10.1.4",
                            "nodemon":  "^3.1.10",
                            "supertest":  "^6.3.4"
                        },
    "author":  "Your Name",
    "license":  "ISC"
}
